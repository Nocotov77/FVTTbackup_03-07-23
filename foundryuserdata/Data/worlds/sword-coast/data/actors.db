im":false,"fly":false,"burrow":false,"climb":false},"mountain":{"any":false,"walk":false,"swim":false,"fly":false,"burrow":true,"climb":true},"swamp":{"any":false,"walk":false,"swim":false,"fly":false,"burrow":false,"climb":false},"underdark":{"any":false,"walk":false,"swim":false,"fly":false,"burrow":false,"climb":false},"urban":{"any":false,"walk":false,"swim":false,"fly":false,"burrow":false,"climb":false},"water":{"any":false,"walk":false,"swim":true,"fly":false,"burrow":false,"climb":false}}},"healthEstimate":{"dontMarkDead":false,"hideHealthEstimate":false,"hideName":false},"splatter":{"bloodColor":""}},"randomImg":false},"items":[{"name":"Дампир","type":"feat","img":"modules/laaru-dnd5-hw/assets/images/pic/race_dhampir.webp","effects":[{"_id":"xq9gS3vHdqJiLoul","changes":[{"key":"system.details.race","mode":2,"value":"Дампир","priority":20},{"key":"system.traits.languages.value","mode":2,"value":"common","priority":20},{"key":"system.attributes.movement.walk","mode":0,"value":"35","priority":20}],"disabled":false,"duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"modules/laaru-dnd5-hw/assets/images/pic/race_dhampir.webp","label":"Особенности дампира","origin":"Item.v1K4d2p0aEjhhhf2","transfer":true,"flags":{"dae":{"selfTarget":false,"stackable":"none","durationExpression":"","specialDuration":[],"transfer":true},"core":{"statusId":""}},"tint":null}],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Item.v1K4d2p0aEjhhhf2"},"variant-encumbrance-dnd5e":{"bulk":0},"magicitems":{"enabled":false,"equipped":false,"attuned":false,"charges":"0","chargeType":"c1","destroy":false,"destroyFlavorText":"reaches 0 charges: it crumbles into ashes and is destroyed.","rechargeable":false,"recharge":"0","rechargeType":"t1","rechargeUnit":"r1","sorting":"l"}},"_id":"YXMWV7CLl5GrwZ9L","system":{"description":{"value":"<details><summary><strong>Описание дампиров </strong><em>(нажмите сюда, чтобы прочитать)</em></summary><p>Находясь между мирами живых и мёртвых, дампиры сохраняют свою власть над жизнью, но подвергаются бесконечным испытаниям жестоким голодом. Их связи с нежитью дают дампирам возможность ощутить удаль бессмертного вампира в виде повышенной скорости, ночного зрения и истощающего жизнь укуса. Обладая уникальным пониманием природы нежити, многие дампиры становятся искателями приключений и охотниками на монстров. Их причины часто очень личные.</p><p>Некоторые ищут опасности, представляя что монстры — это олицетворения их собственного голода. Другие мстят тому, что превратило их в дампира. А третьи принимают уединение охоты, стремясь дистанцироваться от тех, кто искушает свой голод.</p><h2>Происхождения дампиров</h2><p>Дампиры часто возникают в результате встреч с вампирами, но всевозможные жуткие сделки, некромантские влияния и встречи с таинственными бессмертными могли изменить Вашего персонажа. В таблице «Происхождения дампиров» содержатся предположения о том, как Ваш персонаж получил своё наследие.</p><table style=\"width:100%\" border=\"0\"><tbody><tr><td style=\"width:11.1215%\"><strong>[[/r d8]]</strong></td><td style=\"width:88.8785%\"><strong>Происхождение</strong></td></tr><tr><td style=\"width:11.1215%\">1</td><td style=\"width:88.8785%\">Вы — реинкарнация предка, который был вампиром-тираном.</td></tr><tr><td style=\"width:11.1215%\">2</td><td style=\"width:88.8785%\">Ваш договор с хищным божеством, исчадием, феей или духом заставляет Вас разделять их голод.</td></tr><tr><td style=\"width:11.1215%\">3</td><td style=\"width:88.8785%\">Вы пережили нападение вампира, но навсегда изменились.</td></tr><tr><td style=\"width:11.1215%\">4</td><td style=\"width:88.8785%\">Внутри Вас живет паразит. Вы утоляете его голод.</td></tr><tr><td style=\"width:11.1215%\">5</td><td style=\"width:88.8785%\">Трагедия прервала ваше превращение в бессмертного.</td></tr><tr><td style=\"width:11.1215%\">6</td><td style=\"width:88.8785%\">Вы — уменьшенная форма потустороннего существа. Утоление голода ускоряет ваше обновление.</td></tr><tr><td style=\"width:11.1215%\">7</td><td style=\"width:88.8785%\">Один из Ваших родителей был вампиром.</td></tr><tr><td style=\"width:11.1215%\">8</td><td style=\"width:88.8785%\">Радикальный эксперимент изменил ваше тело, заставив полагаться на других в получении жизненно важных жидкостей.</td></tr></tbody></table><h2>Дампиры в Доменах Ужаса</h2><p>Создавая дампира, спросите своего Мастера о том, имеет ли смысл создать для Вашего персонажа связь с одним из следующих Доменов Ужаса</p><p><strong>Баровия. </strong>В тени замка Равенлофт расцветают истории о тех, кто любит вампиров или произошёл от них. У вас может быть такая связь, но бойтесь того, что произойдёт, если прочие члены вашей изолированной общины об этом узнают.</p><p><strong>Даркон. </strong>Каргат, тайная полиция этого расколотого владения, якобы знает секрет бессмертия. Возможно, вы вступили в неё и дослужились до ранга, на котором вас наградили превращением в дампира.</p><p><strong>Ай’Кат. </strong>Ночью голодный город Ай’Кат наводняют вампирские цзянши. После встречи с одним из этих беспокойных предков вы остались с холодным голодом внутри.</p><h2>Наследие</h2><p>В Земле Туманов сила и ужас заключаются в простом вопросе: «Что со мной случилось?».</p><p>Наследие Дампир — это раса, которую персонажи могут получить в результате знаменательных событий. Они затмевают первоначальную расу, если таковая имеется, становясь их новой расой. Вы можете выбрать наследие во время создания персонажа, и тогда его трансформация произошла до начала игры. Или же события могут разворачиваться во время приключений, которые приведут вашего персонажа к замене своей расы наследием. Поработайте со своим Мастером, чтобы определить, ответственны ли вы за свою трансформацию и то, как история повернулась после этого события.</p><p>Наследия представляют физические и магические трансформации, которые фундаментально изменили вас. Вы можете всё ещё выглядеть как раньше, но значительно изменяетесь, что может полностью поменять ваши физические и магические способности.</p><p>Например, драконорождённый, став дампиром, теряет свою связь с драконьим наследием из-за бессмертной вампирской энергии, плещущейся внутри него. Вместо смертельного дыхания, он теперь чувствует внутри себя мощный голод, а его клыки теперь могут высасывать чужую жизнь.</p><p>Некоторые расовые особенности могут остаться нетронутыми при получении наследия, как указано в особенности «Наследие предков». Учитывайте это, когда будете изучать подробности того, как именно наследие изменило вашего персонажа.</p><h2><span id=\"imena_aarakokr\">Голод дампиров</span></h2><p> У каждого дампира есть жажда, которую могут утолить только живые. Те, кто злоупотребляет этим, рискуют потерять контроль и навсегда начать видеть в окружающих только добычу. Те же, кто сопротивляется, могут найти исключительные способы контролировать свои побуждения или подавлять их постоянным мучительным сдерживанием. В любом случае искушение преследует дампиров, и обстоятельства будто складываются так, чтобы дать им бесконечные поводы для насыщения. Хотя многие дампиры жаждут крови, ваш персонаж мог бы получать иное пропитание от живых. <br />Определите Ваш голод по таблице «Голод дампиров», или же самостоятельно выберите то, что заставляет Вашего персонажа кормиться.</p><table style=\"width:100%\" border=\"0\"><tbody><tr style=\"height:17px\"><td style=\"width:11.0874%;height:17px\">[[/r d6]]</td><td style=\"width:88.9126%;height:17px\">Голод</td></tr><tr style=\"height:17px\"><td style=\"width:11.0874%;height:17px\">1</td><td style=\"width:88.9126%;height:17px\">Кровь</td></tr><tr style=\"height:17px\"><td style=\"width:11.0874%;height:17px\">2</td><td style=\"width:88.9126%;height:17px\">Плоть или сырое мясо</td></tr><tr style=\"height:17px\"><td style=\"width:11.0874%;height:17px\">3</td><td style=\"width:88.9126%;height:17px\">Спинномозговая жидкость</td></tr><tr style=\"height:17px\"><td style=\"width:11.0874%;height:17px\">4</td><td style=\"width:88.9126%;height:17px\">Психическая энергия</td></tr><tr style=\"height:17px\"><td style=\"width:11.0874%;height:17px\">5</td><td style=\"width:88.9126%;height:17px\">Сны</td></tr><tr style=\"height:17px\"><td style=\"width:11.0874%;height:17px\">6</td><td style=\"width:88.9126%;height:17px\">Жизненная сила</td></tr></tbody></table></details><h2 style=\"border-bottom:1px solid var(--color-underline-header)\"><span style=\"font-family:Inter, Arial, sans-serif\">Особенности дампиров</span></h2><table class=\"w-100 summary stripe-even\" style=\"width:100%\"><tbody><tr><th class=\"col-4 text-center\" style=\"width:28.0844%\">Характеристики</th><th class=\"col-4 text-center\" style=\"width:18.8853%\">Размер</th><th class=\"col-4 text-center\" style=\"width:53.0303%\">Скорость</th></tr><tr><td class=\"text-center\" style=\"width:28.0844%\"><p>Выберите одно из следующих:</p><p>(а) Значение одной характеристики по вашему выбору увеличивается на 2, а другой — на 1; <br />(б) Значение трёх различных характеристик по вашему выбору увеличивается на 1.</p></td><td class=\"text-center\" style=\"width:18.8853%\">Небольшой/Средний</td><td class=\"text-center\" style=\"width:53.0303%\">35 футов</td></tr></tbody></table><section><section><section class=\"secret revealed\" id=\"secret-IsW0fLCM34FNDvqg\"><p>Добавьте в свой бланк персонажа Особенности расы вручную из этого окна.</p></section></section><p><em><strong>Наследие предков.</strong></em> Если Вы заменяете свою расу на это наследие, то можете сохранить следующие особенности той расы:</p><ul><li>Умение в навыках;</li><li>Скорость лазания, полёта, плавания.</li></ul><p>Если Вы не сохраняете никакие из этих особенностей или если вы выбрали это наследие при создании персонажа, то вы получаете умение в двух навыках на ваш выбор.</p><p><em><strong>Вид существа</strong></em>. Вы Гуманоид. </p><p><em><strong>Размер</strong></em>. Ваш @Compendium[laaru-dnd5-hw.conditions.sDPntYBdmN7jMZc6]{размер} — Небольшой или Средний. Вы выбираете размер, когда получаете это наследие.</p><p><em><strong>Скорость</strong></em>. Ваша базовая скорость ходьбы — 35 футов.</p><p><span style=\"font-family:var(--font-primary)\"><em><strong style=\"font-family:var(--font-primary);font-size:var(--font-size-14)\">@Compendium[laaru-dnd5-hw.classfeatures.cStJIRd72oj7r2fM]{Ночное зрение}</strong></em><strong style=\"font-family:var(--font-primary);font-size:var(--font-size-14)\">.</strong></span> На расстоянии в 60 футов вы при  тусклом свете можете видеть так, как будто это  яркий свет, и в  темноте так, как будто это тусклое освещение. В темноте вы не можете различать цвета, только оттенки серого.</p><p><em><strong>@Compendium[laaru-dnd5-hw.classfeatures.znINpFuoFGUw48JD]{Вампирский укус}.</strong></em> Ваш Укус клыками — природное оружие, считающееся простым оружием ближнего боя, в котором вы умелы.<br />Вы добавляете к атакам и урону Укуса модификатор Выносливости вместо модификатора Силы.<br />Клыки наносят 1d4 колющего урона при попадании.<br />Когда у вас половина ПЗ или меньше, вы получаете преимущество на атаки Клыками.<br />Попадая атакой Клыками по существу, не являющемуся конструкцией или нежитью, вы усиливаете себя одним из следующим способов на ваш выбор.</p><ul><li>Вы восстанавливаете количество ПЗ, равное колющему урону, нанесённому Укусом клыками.</li><li>Вы получаете бонус на следующую проверку способности или бросок атаки. Бонус равен колющему урону, нанесённому Укусом клыками.</li></ul><p>Вы можете усиливать себя Укусом количество раз, равное вашему бонусу умения, и восстанавливаете все использования, заканчивая долгий отдых.</p><p><em><strong>@Compendium[laaru-dnd5-hw.classfeatures.rRS9wxOkYjDwCiPu]{Паучьи лапы}. </strong></em>Вы получаете скорость лазания, равную вашей скорости ходьбы.<br />Кроме того, начиная с 3 уровня, Вы можете передвигаться вверх, вниз и вдоль вертикальных поверхностей, а также вверх ногами по потолку, оставляя руки свободными.</p><p><em><strong>@Compendium[laaru-dnd5-hw.classfeatures.280dwVcNikrfQhLJ]{Бессмертная природа}.</strong></em> Вам не нужно дышать.</p><p><span style=\"font-family:var(--font-primary)\"><em><strong style=\"font-family:var(--font-primary);font-size:var(--font-size-14)\">Языки</strong></em>.</span> Ваш персонаж может говорить, читать и писать на всеобщем и еще одном языке, который по вашему мнению и мнению мастера подходит для персонажа.</p></section>","chat":"","unidentified":""},"source":"VRGR","activation":{"type":"","cost":0,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"","subtype":""},"requirements":"","recharge":{"value":null,"charged":false}},"ownership":{"default":0,"ekSJaSkR4p1sowmM":3,"qaQCG0lQ1MdG3tft":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1678986205158,"modifiedTime":1678986225150,"lastModifiedBy":"qaQCG0lQ1MdG3tft"}},{"_id":"YhEXHS97NwwlCqLb","name":"Солдат (PHB)","type":"background","img":"icons/environment/people/spearfighter.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.laaru-dnd5-hw.backgrounds.L6ElRlLNDgSwfHDa"}},"system":{"description":{"value":"<p>Вы воевали всю жизнь, сколько себя помните. С юности вы учились обращаться с оружием и бронёй и выживать на поле боя. Возможно, вы служили в регулярной армии своей страны или в отряде наёмников, а может, вы местный ополченец, который проявил себя в недавней войне. </p><p>Выбрав это происхождение, проработайте со своим мастером, в каком войске вы служили, до какого звания дослужились и что пережили во время службы. Была ли это регулярная армия, городская стража или деревенское ополчение? А может, это был отряд наёмников или личная гвардия аристократа или купца?</p><hr style=\"border-top:1px solid var(--color-border-light-primary);border-bottom:1px solid var(--color-border-light-highlight)\" /><p><strong>Умения в навыках:</strong> Атлетика, Запугивание.</p><p><strong>Умение в инструментах</strong>: один вид @UUID[Compendium.laaru-dnd5-hw.items.EuJni6mHO5GBqPHn]{настольных игр}, транспорт (наземный).</p><p><strong>Снаряжение</strong>: воинский знак отличия, трофей, взятый у павшего врага (@Compendium[dnd5e.items.0E565kQUBmndJ1a2]{кинжал}, сломанный клинок или обрывок знамени), @Compendium[dnd5e.items.iBuTM09KD9IoM5L8]{игровые кости} или @Compendium[dnd5e.items.YwlHI3BVJapz4a3E]{колода карт}, @Compendium[dnd5e.items.8RXjiddJ6VGyE7vB]{повседневная одежда}, @Compendium[dnd5e.items.9bWTRRDym06PzSAf]{кошель} с 10 зм.</p><p><strong>Особенность:</strong></p><p>@Compendium[laaru-dnd5-hw.classfeatures.EGPKde5MHY274CyV]{Воинское звание}</p><hr /><h3><strong>Специальность</strong></h3><p>Во время службы вы исполняли определённые обязанности в отряде или армии. Выберите или определите броском <a title=\"d8\" data-mode=\"roll\" data-flavor data-formula=\"d8\"> d8</a> один из вариантов из таблицы ниже.</p><p>@Compendium[laaru-dnd5-hw.tables.oPCdyXG4nSIYi6I2]{Специальность солдата}</p><hr style=\"border-top:1px solid var(--color-border-light-primary);border-bottom:1px solid var(--color-border-light-highlight)\" /><h3>Рекомендуемые личные качества</h3><p>Ужас войны и строгая дисциплина военной службы оставили отпечаток на идеалах солдат, на крепких узах между ними, а зачастую и шрамы на их душах, сделав их уязвимыми перед страхом, позором и ненавистью.</p><p>@Compendium[laaru-dnd5-hw.tables.eMQPGREmSW1uqkMW]{Свойства личности}</p><p>@Compendium[laaru-dnd5-hw.tables.FaixjETnvZoGhD1C]{Идеалы}</p><p>@Compendium[laaru-dnd5-hw.tables.YWHFRAmfWuFYKeqP]{Узы}</p><p>@Compendium[laaru-dnd5-hw.tables.9S5mArWwCzzUTqC7]{Изъяны}</p>","chat":"","unidentified":""},"source":"Player Handbook","advancement":[{"_id":"m59y2btlkvgzohz5","type":"ItemGrant","configuration":{"items":["Compendium.laaru-dnd5-hw.classfeatures.EGPKde5MHY274CyV"],"optional":false,"spell":{"ability":"","preparation":"","uses":{"max":"","per":""}}},"value":{"added":{"8RvKcTYD2Oc6TqjW":"Compendium.laaru-dnd5-hw.classfeatures.EGPKde5MHY274CyV"}},"level":0,"title":"Особенность"},{"_id":"5vgzkkoeabfd0yhz","type":"ItemGrant","configuration":{"items":["Compendium.dnd5e.items.0E565kQUBmndJ1a2","Compendium.dnd5e.items.iBuTM09KD9IoM5L8","Compendium.dnd5e.items.YwlHI3BVJapz4a3E","Compendium.dnd5e.items.8RXjiddJ6VGyE7vB","Compendium.dnd5e.items.9bWTRRDym06PzSAf"],"optional":true,"spell":{"ability":"","preparation":"","uses":{"max":"","per":""}}},"value":{"added":{"21WPrGsOwqTZRQbU":"Compendium.dnd5e.items.0E565kQUBmndJ1a2","Cjl3CP3ObBTnSCjj":"Compendium.dnd5e.items.iBuTM09KD9IoM5L8","26ITM5FMQMnHQUiW":"Compendium.dnd5e.items.YwlHI3BVJapz4a3E","0JjNi6cyHKjOkAM8":"Compendium.dnd5e.items.8RXjiddJ6VGyE7vB","dsq2zmAyQmxuiuCA":"Compendium.dnd5e.items.9bWTRRDym06PzSAf"}},"level":0,"title":"Снаряжение солдата"}]},"ownership":{"default":0,"ekSJaSkR4p1sowmM":3,"3NAHMw7i4GZu1kon":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1678987978155,"modifiedTime":1683230760763,"lastModifiedBy":"3NAHMw7i4GZu1kon"}},{"_id":"8RvKcTYD2Oc6TqjW","name":"Воинское звание","type":"feat","img":"icons/sundries/flags/banner-green.webp","effects":[{"_id":"bKEaTw8Srg7oZeYL","changes":[{"key":"system.skills.ath.value","mode":2,"value":"1","priority":20},{"key":"system.skills.itm.value","mode":2,"value":"1","priority":20}],"disabled":false,"duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"icons/environment/people/infantry.webp","label":"Особенности солдата","origin":"Item.EGPKde5M